{
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": ["OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://dashboard-aksara-fix.vercel.app",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH",
        "Access-Control-Max-Age": "86400"
      },
      "dest": "api/index.js"
    },

    {
      "src": "/api/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
      "headers": {
        "Access-Control-Allow-Origin": "https://dashboard-aksara-fix.vercel.app"
      },
      "dest": "api/index.js"
    },

    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
